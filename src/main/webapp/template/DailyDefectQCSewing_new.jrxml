	COALESCE ( SUM ( cosmetic_no_nik ), 0 ) AS cosmetic_no_nik,
	COALESCE ( SUM ( cosmetic_no_pairing ), 0 ) AS cosmetic_no_pairing,
	-- end cosmetic --
	COALESCE ( SUM ( other_sockliner ), 0 ) AS other_sockliner,
	COALESCE ( SUM ( other_doc_date ), 0 ) AS other_doc_date,
	COALESCE ( SUM ( other_others ), 0 ) AS other_others,
	( SELECT SUM ( qty ) FROM jdcadp_mstproduksi WHERE demand_class = $P{demand_class} AND line_code = $P{line_code} AND jo_ppic = $P{product_code} ) AS output_qc
FROM
     view_qc_assembly
WHERE
     tgl = $P{tgl}
 AND jam >= '17.30'
 AND jam < '18:30'
 AND demand_class = $P{demand_class}
 AND line_code = $P{line_code}
 AND product_code = $P{product_code}
AND area = $P{userType}
UNION
